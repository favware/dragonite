{
  "name": "dragonite",
  "version": "1.0.0",
  "description": "A Pok√©mon information Discord bot built around Discord Interactions.",
  "author": "Jeroen Claassens <support@favware.tech>",
  "license": "Apache-2.0",
  "main": "dist/dragonite.js",
  "private": true,
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/util/*.js",
    "#gql/*": "./dist/lib/gql/*.js",
    "#lib/env": "./dist/lib/env/index.js"
  },
  "scripts": {
    "start": "node --preserve-symlinks --enable-source-maps dist/dragonite.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "node scripts/clean.mjs",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@favware/graphql-pokemon": "^6.3.0",
    "@influxdata/influxdb-client": "^1.22.0",
    "@influxdata/influxdb-client-apis": "^1.22.0",
    "@sapphire/decorators": "^4.0.2",
    "@sapphire/discord-utilities": "^2.6.0",
    "@sapphire/discord.js-utilities": "^4.4.0",
    "@sapphire/fetch": "^2.0.4",
    "@sapphire/framework": "next",
    "@sapphire/plugin-logger": "^2.1.2",
    "@sapphire/plugin-scheduled-tasks": "^2.1.0",
    "@sapphire/stopwatch": "^1.2.5",
    "@sapphire/time-utilities": "^1.5.2",
    "@sapphire/type": "^2.1.2",
    "@sapphire/utilities": "^3.2.1",
    "binarytf": "^2.1.0",
    "bufferutil": "^4.0.6",
    "bull": "^4.2.1",
    "colorette": "^2.0.16",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv-cra": "^3.0.2",
    "ioredis": "^4.28.3",
    "utf-8-validate": "^5.0.8",
    "ws": "^8.4.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@sapphire/eslint-config": "^4.0.11",
    "@sapphire/prettier-config": "^1.2.9",
    "@sapphire/ts-config": "^3.1.8",
    "@types/bull": "^3.15.7",
    "@types/ioredis": "^4.28.7",
    "@types/node": "^17.0.10",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/dragonite.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "bot",
    "autocomplete",
    "pokemon",
    "discord",
    "buttons",
    "slash-commands",
    "slash",
    "sapphire",
    "skyra",
    "favware",
    "select-menu",
    "favna",
    "slashies"
  ],
  "packageManager": "yarn@3.1.1"
}
