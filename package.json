{
  "name": "dragonite",
  "version": "1.0.0",
  "description": "A Pok√©mon information Discord bot built around Discord Interactions.",
  "author": "Jeroen Claassens <support@favware.tech>",
  "license": "Apache-2.0",
  "main": "dist/dragonite.js",
  "private": true,
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/util/*.js",
    "#gql/*": "./dist/lib/gql/*.js",
    "#lib/env": "./dist/lib/env/index.js"
  },
  "scripts": {
    "start": "node --preserve-symlinks --enable-source-maps dist/dragonite.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "node scripts/clean.mjs",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\""
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@favware/graphql-pokemon": "^6.5.0",
    "@influxdata/influxdb-client": "^1.24.0",
    "@influxdata/influxdb-client-apis": "^1.24.0",
    "@sapphire/decorators": "^4.3.3",
    "@sapphire/discord-utilities": "^2.10.2",
    "@sapphire/discord.js-utilities": "^4.9.3",
    "@sapphire/fetch": "^2.3.0",
    "@sapphire/framework": "next",
    "@sapphire/plugin-logger": "^2.2.0",
    "@sapphire/plugin-scheduled-tasks": "^2.3.0",
    "@sapphire/snowflake": "^3.2.0",
    "@sapphire/stopwatch": "^1.4.0",
    "@sapphire/time-utilities": "^1.7.3",
    "@sapphire/type": "^2.2.0",
    "@sapphire/utilities": "^3.6.1",
    "binarytf": "^2.1.1",
    "bufferutil": "^4.0.6",
    "bull": "^4.8.1",
    "colorette": "^2.0.16",
    "discord-api-types": "0.26.1",
    "discord.js": "^13.6.0",
    "dotenv-cra": "^3.0.2",
    "figlet": "^1.5.2",
    "gradient-string": "^2.0.0",
    "ioredis": "^4.28.5",
    "utf-8-validate": "^5.0.9",
    "ws": "^8.5.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@sapphire/eslint-config": "^4.3.2",
    "@sapphire/prettier-config": "^1.4.1",
    "@sapphire/ts-config": "^3.3.3",
    "@swc/core": "^1.2.160",
    "@types/bull": "^3.15.8",
    "@types/figlet": "^1.5.4",
    "@types/gradient-string": "^1.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^17.0.21",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@discordjs/builders": "^0.12.0"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/dragonite.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "bot",
    "autocomplete",
    "pokemon",
    "discord",
    "buttons",
    "slash-commands",
    "slash",
    "sapphire",
    "skyra",
    "favware",
    "select-menu",
    "favna",
    "slashies"
  ],
  "packageManager": "yarn@3.2.0"
}
