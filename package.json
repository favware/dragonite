{
  "name": "dragonite",
  "version": "1.0.0",
  "description": "A Pok√©mon information Discord bot built around Discord Interactions.",
  "author": "Jeroen Claassens <support@favware.tech>",
  "license": "Apache-2.0",
  "main": "dist/dragonite.js",
  "private": true,
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/util/*.js",
    "#gql/*": "./dist/lib/gql/*.js",
    "#lib/env": "./dist/lib/env/index.js"
  },
  "scripts": {
    "start": "node --preserve-symlinks --enable-source-maps dist/dragonite.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "node scripts/clean.mjs",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\"",
    "postinstall": "husky install .github/husky"
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@favware/graphql-pokemon": "^6.5.12",
    "@influxdata/influxdb-client": "^1.27.0",
    "@influxdata/influxdb-client-apis": "^1.27.0",
    "@sapphire/decorators": "^4.3.6",
    "@sapphire/discord-utilities": "^2.11.4",
    "@sapphire/discord.js-utilities": "^4.11.3",
    "@sapphire/fetch": "^2.4.0",
    "@sapphire/framework": "next",
    "@sapphire/plugin-logger": "^2.2.1",
    "@sapphire/plugin-scheduled-tasks": "^2.3.3",
    "@sapphire/snowflake": "^3.2.2",
    "@sapphire/stopwatch": "^1.4.1",
    "@sapphire/time-utilities": "^1.7.4",
    "@sapphire/type": "^2.2.4",
    "@sapphire/utilities": "^3.7.0",
    "@skyra/env-utilities": "^1.0.1",
    "@skyra/start-banner": "^1.0.1",
    "binarytf": "^2.1.1",
    "bufferutil": "^4.0.6",
    "bull": "^4.8.4",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.33.5",
    "discord.js": "^13.8.1",
    "figlet": "^1.5.2",
    "gradient-string": "^2.0.1",
    "ioredis": "^4.28.5",
    "utf-8-validate": "^5.0.9",
    "ws": "^8.8.1",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@sapphire/eslint-config": "^4.3.7",
    "@sapphire/prettier-config": "^1.4.3",
    "@sapphire/ts-config": "^3.3.4",
    "@swc/core": "^1.2.215",
    "@types/bull": "^3.15.8",
    "@types/figlet": "^1.5.4",
    "@types/gradient-string": "^1.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "minimist": "^1.2.6"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/dragonite.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "bot",
    "autocomplete",
    "pokemon",
    "discord",
    "buttons",
    "slash-commands",
    "slash",
    "sapphire",
    "skyra",
    "favware",
    "select-menu",
    "favna",
    "slashies"
  ],
  "packageManager": "yarn@3.2.1"
}
