{
  "name": "dragonite",
  "version": "1.0.0",
  "description": "A Pok√©mon information Discord bot built around Discord Interactions.",
  "author": "Jeroen Claassens <support@favware.tech>",
  "license": "Apache-2.0",
  "main": "dist/dragonite.js",
  "private": true,
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#root/*": "./dist/*.js",
    "#utils/*": "./dist/lib/util/*.js",
    "#gql/*": "./dist/lib/gql/*.js",
    "#lib/env": "./dist/lib/env/index.js"
  },
  "scripts": {
    "start": "node --preserve-symlinks --enable-source-maps dist/dragonite.js",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write .",
    "build": "tsup",
    "watch": "tsup --watch",
    "clean": "rimraf dist/",
    "typecheck": "tsc -b src",
    "dev": "tsup --watch --onSuccess \"yarn start\"",
    "postinstall": "husky install .github/husky"
  },
  "dependencies": {
    "@discordjs/builders": "^1.6.1",
    "@favware/graphql-pokemon": "^7.2.1",
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@sapphire/decorators": "^6.0.1",
    "@sapphire/discord.js-utilities": "^6.0.5",
    "@sapphire/duration": "^1.1.0",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.4.0",
    "@sapphire/plugin-logger": "^3.0.3",
    "@sapphire/plugin-scheduled-tasks": "^6.0.1",
    "@sapphire/stopwatch": "^1.5.0",
    "@sapphire/type": "^2.3.0",
    "@sapphire/utilities": "^3.11.1",
    "@skyra/env-utilities": "^1.2.0",
    "@skyra/jaro-winkler": "^1.1.0",
    "@skyra/start-banner": "^2.0.0",
    "binarytf": "^2.1.3-next",
    "bufferutil": "^4.0.7",
    "bullmq": "^3.10.4",
    "colorette": "^2.0.19",
    "discord-api-types": "^0.37.38",
    "discord.js": "^14.9.0",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "ioredis": "^5.3.2",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.13.0",
    "zlib-sync": "^0.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@sapphire/eslint-config": "^4.4.1",
    "@sapphire/prettier-config": "^1.4.5",
    "@sapphire/ts-config": "^4.0.0",
    "@swc/core": "^1.3.51",
    "@types/figlet": "^1.5.5",
    "@types/gradient-string": "^1.1.2",
    "@types/node": "^18.15.11",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "minimist": "^1.2.8"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/dragonite.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "@sapphire"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "bot",
    "autocomplete",
    "pokemon",
    "discord",
    "buttons",
    "slash-commands",
    "slash",
    "sapphire",
    "skyra",
    "favware",
    "select-menu",
    "favna",
    "slashies"
  ],
  "packageManager": "yarn@3.5.0"
}
